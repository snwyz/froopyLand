{
  "name": "vanilla",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:ts": "eslint src/** --ext .ts,.tsx",
    "lint:ts:fix": "eslint src/** --ext .ts,.tsx --fix"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix",
      "git add ."
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './tools/commitlint.js'",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "tools/commitizen.js"
    }
  },
  "dependencies": {
    "@alch/alchemy-sdk": "^1.2.0",
    "@chakra-ui/icons": "^2.0.12",
    "@chakra-ui/react": "^2.4.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/error-message": "^2.0.1",
    "@react-three/fiber": "^8.9.1",
    "@reduxjs/toolkit": "^1.8.3",
    "@tanstack/react-query": "^4.24.4",
    "@types/chart.js": "^2.9.37",
    "@walletconnect/web3-provider": "^1.8.0",
    "alchemy-sdk": "^2.4.3",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.1",
    "chart.js": "^4.2.0",
    "clsx": "^1.2.1",
    "connectkit": "^1.0.0",
    "date-fns": "^2.29.0",
    "ethers": "^5.7.2",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.4",
    "nanoid": "^4.0.0",
    "next": "^12.3",
    "nookies": "^2.5.2",
    "pretty-num": "^0.5.2",
    "qs": "^6.11.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.40.0",
    "react-lazy-load-image-component": "^1.5.6",
    "react-modal": "^3.15.1",
    "react-paginate": "^8.1.4",
    "react-redux": "^8.0.2",
    "react-responsive": "^9.0.0-beta.10",
    "react-toastify": "^9.1.1",
    "react-vh": "^1.1.1",
    "three": "^0.146.0",
    "wagmi": "^0.8.4",
    "walletlink": "^2.5.0",
    "web3": "^1.8.1",
    "web3modal": "^1.9.10",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@types/axios": "^0.14.0",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "commitizen": "^4.2.5",
    "cz-customizable": "^6.9.1",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "hex2dec": "^1.1.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1"
  }
}
